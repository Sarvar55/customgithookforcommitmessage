{
    "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
    "globals": {
      "React": "readonly"
    },
    "rules": {
      "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }],
      "camelcase": ["error", { "properties": "always" }],
      "semi": "error",
      "no-undef": "error",
      "no-duplicate-imports": "error",
      "func-style": ["error", "expression"],
      "func-names": "error"
    }
  }